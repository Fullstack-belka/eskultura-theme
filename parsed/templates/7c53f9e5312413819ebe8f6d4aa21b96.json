div>\r\n        </li>\r\n\r\n          {{>components/common/currency-selector}}\r\n\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navUser-item user-icons gift-certificates\">\r\n                <a title=\"Gift Certificates\" alt=\"Gift Certificates\" class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"><svg class=\"header-icon gift-icon\"><use xlink:href=\"#icon-gift\" /></svg></a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        <a title=\"Compare Items\" class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav><svg class=\"header-icon compare-icon\"><use xlink:href=\"#icon-compare\"></use></svg><span class=\"countPill countPill--positive countPill--alt compare\"></span></a>\r\n\r\n        <!-- <li class=\"navUser-item navUser-item--account user-icons account\"> -->\r\n                {{#if settings.account_creation_enabled}}\r\n                    <!-- <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a> -->\r\n                    {{> components/common/account-link}}\r\n                {{/if}}\r\n        <!-- </li> -->\r\n\r\n        <li class=\"navUser-item quicksearch mobile\">\r\n            <a class=\"navUser-action navUser-action--quickSearch desktop-toggle\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><svg class=\"header-icon search-icon\"><use xlink:href=\"#icon-search\"></use></svg></a>\r\n        </li>\r\n\r\n    </ul>\r\n    </div>\r\n\r\n</nav>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n    <div class=\"flyout-pagination-container top\">\n      <a class=\"navPages-action previous