{"components/common/mega-nav-page-list":"<div class=\"nav-page-list mega-nav-page-list\">\n{{#if children}}\n<a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent topLevel sideNavTopLevel\" href=\"{{url}}\">\n    {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    <i class=\"icon navPages-action-moreIcon fixed-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n</a>\n<div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child second-level\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu second-level\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{id}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}} </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action second-level\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n    